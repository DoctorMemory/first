<h1>New team</h1>

<% form_for(@team) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :wins %><br />
    <%= f.text_field :wins %>
  </p>
  <p>
    <%= f.label :losses %><br />
    <%= f.text_field :losses %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', teams_path %>